{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "raw": {
      "type": "number",
      "default": 0
    },
    "pausedTime": {
      "anyOf": [{ "type": "number" }, { "type": "null" }],
      "default": null
    },
    "display": {
      "type": "number",
      "default": 0
    },
    "results": {
      "type": "array",
      "items": {
        "anyOf": [{ "type": "number" }, { "type": "null" }]
      },
      "default": [0, 0, 0, 0, 0, 0, 0]
    },
    "completeCount": {
      "type": "number",
      "default": 0
    },
    "state": {
      "enum": ["Finished", "Running", "Stopped"],
      "type": "string",
      "default": "Stopped"
    }
  },
  "additionalProperties": false,
  "required": ["completeCount", "display", "pausedTime", "raw", "results", "state"]
}
